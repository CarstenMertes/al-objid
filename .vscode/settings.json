{
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "**/Thumbs.db": true,
        "doc": true,
        "azure-function-app": true,
        "azure-polling-app": true,
        "azure-telemetry-app": true,
        "vscode-extension": true,
        ".vscode": true,
        "al-obj-id.code-workspace": true,
        ".debug": true,
        ".snapshots": true
    },
    "azureFunctions.projectSubpath": "azure-function-app",
