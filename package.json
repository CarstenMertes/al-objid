{
	"name": "vjeko-al-objid",
	"displayName": "Vjeko.com AL Object ID Ninja",
	"description": "Manage object IDs in multi-user environments with mind-boggling simplicity.",
	"publisher": "vjeko-com",
	"version": "0.0.3",
	"repository": {
		"type": "git",
		"url": "https://github.com/vjekob/al-objid.git"
	},
	"icon": "images/icon.png",
	"galleryBanner": {
		"color": "#505C6D",
		"theme": "dark"
	},
	"engines": {
		"vscode": "^1.60.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onLanguage:al",
		"onCommand:vjeko-al-objid.sync-object-ids",
		"onCommand:vjeko-al-objid.authorize-app",
		"onCommand:vjeko-al-objid.deauthorize-app"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "vjeko-al-objid.sync-object-ids",
				"title": "Vjeko: Synchronize Object IDs with Azure Back End"
			},
			{
				"command": "vjeko-al-objid.authorize-app",
				"title": "Vjeko: Authorize AL App"
			},
			{
				"command": "vjeko-al-objid.deauthorize-app",
				"title": "Vjeko: Deauthorize AL App"
			}
		],
		"configuration": {
			"title": "Vjeko.com AL Object ID Ninja",
			"properties": {
				"objectidninja.backEndUrl": {
					"type": "string",
					"markdownDescription": "URL of the back-end API you want to communicate with. This API must support the same functionality as https://github.com/vjekob/al-objid/tree/master/backend"
				},
				"objectidninja.backEndAPIKey": {
					"type": "string",
					"description": "API key used to access the back-end."
				}
			}
		},
		"languages": [
			{
				"id": "jsonc",
				"filenames": [".objidauth"]
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.60.0",
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.2.2",
		"@types/node": "14.x",
		"eslint": "^7.27.0",
		"@typescript-eslint/eslint-plugin": "^4.26.0",
		"@typescript-eslint/parser": "^4.26.0",
		"glob": "^7.1.7",
		"mocha": "^8.4.0",
		"typescript": "^4.3.2",
		"vscode-test": "^1.5.2"
	},
	"dependencies": {
	}
}
