{
  "bindings": [
    {
      "authLevel": "anonymous",
      "type": "httpTrigger",
      "direction": "in",
      "name": "req",
      "methods": [
        "post",
        "delete"
      ],
      "route": "v1/authorizeApp"
    },
    {
      "type": "http",
      "direction": "out",
      "name": "res"
    },
    {
      "type": "blob",
      "direction": "in",
      "name": "ranges",
      "path": "%BlobContainer%/{appId}/_ranges.json"
    }
  ],
  "scriptFile": "../dist/authorizeApp_v1/index.js"
}
