{
  "bindings": [
    {
      "authLevel": "anonymous",
      "type": "httpTrigger",
      "direction": "in",
      "name": "req",
      "methods": [
        "post"
      ],
      "route": "v1/createPool"
    },
    {
      "type": "http",
      "direction": "out",
      "name": "res"
    },
    {
      "type": "blob",
      "direction": "in",
      "name": "pool",
      "path": "%BlobContainer%/{appId}/_pool.json"
    }
  ],
  "scriptFile": "../dist/createPool_v1/index.js"
}
