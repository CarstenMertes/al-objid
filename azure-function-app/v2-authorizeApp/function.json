{
  "bindings": [
    {
      "authLevel": "anonymous",
      "type": "httpTrigger",
      "direction": "in",
      "name": "req",
      "methods": [
        "get",
        "post",
        "delete"
      ],
      "route": "v2/authorizeApp"
    },
    {
      "type": "http",
      "direction": "out",
      "name": "res"
    },
    {
      "type": "queue",
      "direction": "out",
      "name": "notify",
      "queueName": "appchange",
      "connection": "NotifyQueueStorage"
    }
  ],
  "scriptFile": "../dist/src/functions/v2/authorizeApp/index.js"
}
